{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "name": "vorta/phalcon-base",
    "description": "Phalcon Project Base",
    "type": "project",
    "authors": [
        {
            "name": "Vorta",
            "email": "vorta13@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-phalcon": "^3.4",
        "ext-psr": "*",
        "ext-ds": "*"
    },
    "require-dev": {
        "fabfuel/prophiler": "^1.6",
        "phpstan/phpstan": "^0.11.2",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "provide": {
        "psr/log": "^1",
        "psr/cache": "^1",
        "psr/http-message": "^1",
        "psr/container": "^1",
        "psr/link": "^1",
        "psr/http-server-handler": "^1",
        "psr/http-server-middleware": "^1",
        "psr/simple-cache": "^1",
        "psr/http-factory": "^1",
        "psr/http-client": "^1"
    },
    "autoload": {
        "psr-4": {
            "Project\\": "src/"
        }
    }
}
